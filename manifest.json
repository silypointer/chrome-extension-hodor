{
    "manifest_version": 2,
    "name": "Hodor",
    "description":"Hold the door",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
      },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.5.1.min.js"]
        }
      ],
      "browser_action": {
        "default_icon": "hodor.png"
      },
      "permissions":[
        "activeTab"
      ]
  }